{
  "parser": "babel-eslint",
  "extends": "airbnb-base",
  "rules": {
    "comma-dangle": 0,
    "max-len": 0,
    "arrow-parens": ["error", "always"],
    "react/jsx-uses-vars": "error",
    "react/jsx-uses-react": "error",
    "react/sort-prop-types": "error",
    "react/jsx-no-target-blank": "error",
    "react/jsx-indent-props": ["error", 2]
  },
  "plugins": ["react"]
}
