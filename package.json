{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "description": "A boilerplate React application",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run test",
    "build": "webpack --config webpack.config.js --watch",
    "build-for-server": "npm-run-all -p server images build",
    "build:prod": "webpack --config webpack.config.prod.js",
    "build-for-server:prod": "npm-run-all -p build:prod images server",
    "clean": "babel-node script/cleanDist.js",
    "images": "babel-node script/buildImages.js",
    "server": "babel-node script/server.js",
    "open-server": "open https://localhost:3000",
    "dev": "npm-run-all clean build-for-server",
    "prod": "npm-run-all clean build-for-server:prod",
    "start": "npm-run-all dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aem/react-boilerplate.git"
  },
  "author": "Adam Markon <https://github.com/aem>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aem/react-boilerplate/issues"
  },
  "homepage": "https://github.com/aem/react-boilerplate#readme",
  "dependencies": {
    "immutable": "^3.8.1",
    "keymirror": "^0.1.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "5.0.1",
    "react-router": "3.0.0",
    "redux": "3.6.0",
    "redux-thunk": "2.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "compression-webpack-plugin": "^0.3.2",
    "css-loader": "^0.26.1",
    "express": "^4.14.0",
    "ncp": "^2.0.0",
    "node-sass": "^4.0.0",
    "npm-run-all": "^3.1.2",
    "webpack": "2.2.0-rc.1"
  }
}
