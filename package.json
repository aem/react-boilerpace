    "url": "https://github.com/aem/react-boilerplate/issues"
  },
  "homepage": "https://github.com/aem/react-boilerplate#readme",
  "dependencies": {
    "babel-cli": "^6.18.0",
    "css-loader": "^0.26.1",
    "express": "^4.14.0",
    "file-loader": "^0.9.0",
    "immutable": "^3.8.1",
    "keymirror": "^0.1.1",
    "node-sass": "^4.0.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "5.0.1",
    "react-router": "3.0.0",
