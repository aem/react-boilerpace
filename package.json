{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "description": "A boilerplate React application",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run test",
    "build": "webpack --config webpack.config.js",
    "build:prod": "webpack --config webpack.config.prod.js",
