  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "TODO",
    "build:dev:client": "webpack --config webpack/webpack.config.dev.client.babel.js",
    "build:dev:server": "webpack --config webpack/webpack.config.dev.server.babel.js",
    "build:prod:client": "webpack --config webpack/webpack.config.prod.client.babel.js",
    "build:prod:server": "webpack --config webpack/webpack.config.prod.server.babel.js",
    "clean": "babel-node script/cleanBuild.js",
    "server": "babel-node script/startServer.js",
    "prod": "npm-run-all clean build:prod:server build:prod:client server",
    "dev": "npm-run-all clean build:dev:server build:dev:client server",
    "start": "npm-run-all dev",
    "lint": "eslint --ext '.js' ./src"
  },
