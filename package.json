{
  "name": "react-boilerplate",
  "version": "0.1.0",
  "description": "A boilerplate React application",
  "main": "src/index.js",
  "scripts": {
    "build:dev:client": "webpack --config webpack/webpack.config.dev.client.babel.js --watch",
    "build:dev:server": "webpack --config webpack/webpack.config.dev.server.babel.js --watch",
    "build:prod:client": "webpack --config webpack/webpack.config.prod.client.babel.js",
    "build:prod:server": "webpack --config webpack/webpack.config.prod.server.babel.js",
    "clean": "babel-node script/cleanBuild.js",
    "run-server": "babel-node script/startServer.js",
    "run-server-delayed": "npm-run-all delay delay run-server",
    "run-server-watch": "nodemon --watch script --watch src script/startServer.js",
    "run-server-watch-delayed": "npm-run-all delay delay run-server-watch",
    "prod": "npm-run-all -p clean build:prod:server build:prod:client run-server-delayed",
    "dev": "npm-run-all -p clean build:dev:client build:dev:server run-server-watch-delayed",
    "start": "npm-run-all dev",
    "delay": "babel-node script/delay.js 500",
    "test": "mocha test/.setup.js --recursive test --compilers js:babel-register --reporter dot --check-leaks",
    "lint": "eslint --ext '.js' ./src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aem/react-boilerplate.git"
  },
  "author": "Adam Markon <https://github.com/aem>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aem/react-boilerplate/issues"
  },
  "homepage": "https://github.com/aem/react-boilerplate#readme",
  "dependencies": {
    "express": "^4.14.0",
    "immutable": "^3.8.1",
    "keymirror": "^0.1.1",
    "node-sass": "^4.0.0",
    "preact": "^7.1.0",
    "preact-compat": "^3.9.4",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "5.0.1",
    "react-router": "3.0.0",
    "redux": "3.6.0",
    "redux-thunk": "2.1.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.20.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "compression": "^1.6.2",
    "compression-webpack-plugin": "^0.3.2",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-config-airbnb-base": "^11.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx": "^0.0.2",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.8.0",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "mime": "^1.3.4",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^3.1.2",
    "sass-loader": "^4.1.1",
    "style-loader": "^0.13.1",
    "universal-webpack": "^0.2.1",
    "url-loader": "^0.5.7",
    "webpack": "2.2.0-rc.3"
  }
}
